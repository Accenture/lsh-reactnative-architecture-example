{
  "name": "Frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test-unit": "jest",
    "test-coverage": "jest -- --coverage",
    "test": "yarn lint && yarn flow && jest",
    "lint": "esw src/**",
    "lint-watch": "esw -w --changed src/**",
    "flow": "flow",
    "flow-stop": "flow stop",
    "start-ios": "react-native run-ios && npm run lint-watch",
    "start-android": "react-native run-android && npm run lint-watch",
    "pretty": "prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "git add"
    ]
  },
  "dependencies": {
    "es6-symbol": "^3.1.1",
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.3",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "^0.55.4",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera",
    "react-native-code-push": "^5.4.1",
    "react-native-elements": "^0.19.1",
    "react-native-i18n": "^2.0.15",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "23.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-airbnb": "^2.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.0",
    "flow-bin": "^0.67.1",
    "husky": "^0.14.3",
    "jest": "23.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "react-dom": "^16.4.1",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/?(*.)test.js?(x)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest/setupTests.js"
    ]
  }
}
